{
  "name": "Twitch Header Cleaner",
  "version": "1.0",
  "description": "Removes twitch chat header.",
  "manifest_version": 3,
  "permissions": [
    "activeTab",
    "scripting"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "128": "/images/twitch_icon_128.png"
    }
  },
  "icons": {
    "128": "/images/twitch_icon_128.png"
  }
}
